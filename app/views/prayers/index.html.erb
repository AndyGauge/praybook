<% content_for :page_subtitle do %>
	Prayers
<% end %>
<%= render 'new' %>
<% @prayers.each do |prayer| %>
	
		<%= button_to "\u2714", complete_prayer_path(prayer), method: :post %>  
	<%= link_to edit_prayer_path(prayer) do %>
		<%= icon :pencil %>
	<% end %>

	<%= panel prayer.body, heading: prayer.title %>
<% end %>
